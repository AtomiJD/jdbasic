10 REM CONWAY
20 DIM G1(20, 20), G2(20, 20)

30 REM Init
40 FOR I = 1 TO 20
50 FOR J = 1 TO 20
60 G1(I, J) = 0
70 NEXT J
80 NEXT I

90 REM DEF
100 G1(10, 11) = 1
110 G1(11, 12) = 1
120 G1(12, 10) = 1
130 G1(12, 11) = 1
140 G1(12, 12) = 1

150 REM MAIN
160 FOR G = 1 TO 100
170 CLS
180 REM PRINT
190 FOR I = 1 TO 20
200 FOR J = 1 TO 20
210 IF G1(I, J) = 1 THEN PRINT "O";
220 IF G1(I, J) = 0 THEN PRINT ".";
230 NEXT J
240 PRINT
250 NEXT I

260 REM APPL
270 FOR I = 1 TO 20
280 FOR J = 1 TO 20
290 N = G1(I - 1, J - 1) + G1(I - 1, J) + G1(I - 1, J + 1) + G1(I, J - 1) 
295 N = N + G1(I, J + 1) + G1(I + 1, J - 1) + G1(I + 1, J) + G1(I + 1, J + 1)
300 IF G1(I, J) = 1 THEN
305 G2(I, J) = 1
310 IF N < 2 OR N > 3 THEN G2(I, J) = 0
320 ELSE
330 IF N = 3 THEN G2(I, J) = 1 ELSE G2(I, J) = 0
340 END IF
350 NEXT J
360 NEXT I

370 REM COPY 
380 FOR I = 1 TO 20
390 FOR J = 1 TO 20
400 G1(I, J) = G2(I, J)
410 NEXT J
420 NEXT I

430 REM DELAY
440 FOR D = 1 TO 1000: NEXT D

450 NEXT G

460 END
