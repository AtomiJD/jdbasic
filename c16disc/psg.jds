notes[] = [ 120, 127, 135, 143, 152, 160, 170, 180, 191, 203, 215, 227, 240, 255, 270, 287, 304, 320, 341, 360, 383, 405, 429, 455, 479, 509, 541, 573, 607, 640,        682, 720, 766, 810, 859, 910, 958, 1019, 1082, 1147, 1215, 1280, 1364, 1440, 1532, 1621, 1718, 1820, 1917]

func init()
    psgstart
    psgvoice 0, 64, 63, 128, 0
    psgvoice 1, 128, 63, 128, 0
endfunc

init()

for i = 1 to 50
    n = notes[i]
    psgfreq 0, n
    psgfreq 1, n
    psgenv 0, 63, 255, 0, 6
    psgenv 1, 63, 255, 0, 6
